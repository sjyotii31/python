    "import numpy as np\n",
    "x = np.array([5,10,15,40,98,3])\n",
    "def exer1(x):\n",
    "    min_ele = np.min(x)\n",
    "    max_ele = np.max(x)\n",
    "    length = len(x)\n",
    "    add_all = np.sum(x)\n",
    "    avg = add_all / length\n",
    "    return min_ele,max_ele,add_all,avg\n",
    "result = exer1(x)\n",
    "print(result)"
