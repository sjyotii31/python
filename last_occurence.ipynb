    "import numpy as np\n",
    "x = np.array([0,0,1,1,0,0,1])\n",
    "def last_1(x):\n",
    "    index = -1\n",
    "    n = len(x)\n",
    "    for i in range(n):\n",
    "        if x[i] == 1:\n",
    "            index = i\n",
    "    return index\n",
    "result = last_1(x)\n",
    "print(\"Last Occurence od 1:\",result)    "
